{
  "name": "pano-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "generate-config": "node scripts/node/generate-config.ts",
    "dev:config": "npm run generate-config && npm run dev",
    "test:config": "node -e \"const { execSync } = require('child_process'); const os = require('os'); try { const pythonCmd = os.platform() === 'win32' ? 'python' : 'python3'; console.log('Testing configuration generation...'); execSync(`${pythonCmd} scripts/generate_marzipano_config.py --test`, { stdio: 'inherit' }); console.log('Configuration test completed successfully'); } catch (error) { console.error('Configuration test failed:', error.message); process.exit(1); }\"",
    "clean": "rimraf .next out",
    "cleanup-temp": "node scripts/node/cleanup-temp.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:python": "python -m pytest scripts/python/__tests__/generate_config.test.py -v",
    "test:all": "npm run test && npm run test:python",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/formidable": "^3.4.5",
    "axios": "^1.10.0",
    "formidable": "^3.5.4",
    "next": "^14.2.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4",
    "ts-prune": "^0.10.3",
    "typescript": "^5"
  }
}
